    "credentials": true,
    "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS", "HEAD"],
    "headers": ["Content-Type", "Authorization", "X-Frame-Options", "Origin", "Accept"]
  },
  "ip": {
    "enabled": false,
    "whiteList": [],
    "blackList": []
  }
