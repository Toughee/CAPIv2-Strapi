{
  "connection": "default",
  "collectionName": "grreports",
  "info": {
    "name": "grreport",
    "description": "Reporting for Guardian Ruins",
    "endpoint": "/grreports"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "userType": {
      "x-data-description": "CMDR System Type",
      "default": "console",
      "type": "enumeration",
      "required": true,
      "enum": [
        "console",
        "pc"
      ]
    },
    "reportType": {
      "x-data-description": "Report Type",
      "default": "new",
      "type": "enumeration",
      "required": true,
      "enum": [
        "new",
        "update",
        "error"
      ]
    },
    "systemName": {
      "x-data-description": "System Name",
      "type": "string",
      "required": true
    },
    "systemAddress": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "coordX": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "coordY": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "coordZ": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "bodyName": {
      "x-data-description": "Body Name",
      "type": "string",
      "required": true
    },
    "latitude": {
      "x-data-description": "Latitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true,
      "min": -90,
      "max": 90
    },
    "longitude": {
      "x-data-description": "Longitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true,
      "min": -180,
      "max": 180
    },
    "type": {
      "x-data-description": "Type",
      "enum": [
        "Alpha",
        "Beta",
        "Gamma"
      ],
      "type": "enumeration",
      "required": true
    },
    "cmdrName": {
      "x-data-description": "Commander Name",
      "default": "Dr. Arcanonn",
      "type": "string",
      "required": true
    },
    "cmdrComment": {
      "x-data-description": "Comments",
      "type": "text"
    },
    "isBeta": {
      "x-data-render": false,
      "default": false,
      "type": "boolean"
    },
    "screenshot": {
      "x-data-description": "Screenshot",
      "x-data-type": "image",
      "x-data-render": false,
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "x-data-render": false,
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "updated",
        "accepted",
        "declined",
        "issue"
      ]
    },
    "reportComment": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "text"
    },
    "voteCount": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "integer"
    },
    "added": {
      "x-data-render": false,
      "x-data-exclude": true,
      "default": false,
      "type": "boolean"
    },
    "site": {
      "x-data-render": false,
      "x-data-exclude": true,
      "model": "grsite"
    },
    "grobeliskgroupreport": {
      "x-data-render": false,
      "x-data-exclude": true,
      "collection": "grobeliskgroupreport",
      "via": "grreport"
    },
    "grobeliskreport": {
      "x-data-render": false,
      "x-data-exclude": true,
      "collection": "grobeliskreport",
      "via": "grreport"
    }
  }
}
