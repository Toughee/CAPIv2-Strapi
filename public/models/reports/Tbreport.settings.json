{
  "connection": "default",
  "collectionName": "tbreports",
  "info": {
    "name": "tbreport",
    "description": "Reporting for Thargoid Barnacles",
    "endpoint": "/tbreport"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "systemName": {
      "x-data-description": "System Name",
      "type": "string",
      "required": true
    },
    "bodyName": {
      "x-data-description": "Body Name",
      "type": "string",
      "required": true
    },
    "latitude": {
      "x-data-description": "Latitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true,
      "min": -90,
      "max": 90
    },
    "longitude": {
      "x-data-description": "Longitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true,
      "min": -180,
      "max": 180
    },
    "type": {
      "x-data-description": "Type",
      "enum": [
        "Barnacle",
        "Megabarnacle"
      ],
      "type": "enumeration",
      "required": true
    },
    "cycle": {
      "x-data-description": "Cycle",
      "enum": [
        "Unknown",
        "A",
        "B",
        "C",
        "ABC"
      ],
      "type": "enumeration",
      "required": true
    },
    "defense": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "json"
    },
    "count": {
      "x-data-description": "Number of Barnacles",
      "min": 1,
      "type": "integer"
    },
    "cmdrName": {
      "x-data-description": "Commander Name",
      "default": "Dr. Arcanonn",
      "type": "string",
      "required": true
    },
    "cmdrComment": {
      "x-data-description": "Comments",
      "type": "text"
    },
    "isBeta": {
      "x-data-render": false,
      "default": false,
      "type": "boolean"
    },
    "screenshot": {
      "x-data-description": "Screenshot",
      "x-data-type": "image",
      "x-data-render": false,
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "x-data-render": false,
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "updated",
        "accepted",
        "declined",
        "issue"
      ]
    },
    "reportComment": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "text"
    },
    "voteCount": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "integer"
    },
    "added": {
      "x-data-render": false,
      "x-data-exclude": true,
      "default": false,
      "type": "boolean"
    },
    "site": {
      "x-data-render": false,
      "x-data-exclude": true,
      "model": "tbsite"
    }
  }
}
