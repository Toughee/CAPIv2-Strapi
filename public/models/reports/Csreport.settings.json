{
  "connection": "default",
  "collectionName": "csreports",
  "info": {
    "name": "csreport",
    "description": "Reporting for Crystalline Shards",
    "endpoint": "/csreports"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "userType": {
      "x-data-description": "CMDR System Type",
      "default": "console",
      "type": "enumeration",
      "required": true,
      "enum": [
        "console",
        "pc"
      ]
    },
    "reportType": {
      "x-data-description": "Report Type",
      "default": "new",
      "type": "enumeration",
      "required": true,
      "enum": [
        "new",
        "update",
        "error"
      ]
    },
    "systemName": {
      "type": "string",
      "required": true
    },
    "systemAddress": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "regionID": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "integer"
    },
    "coordX": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "coordY": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "coordZ": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "float"
    },
    "bodyName": {
      "x-data-description": "Body Name",
      "type": "string",
      "required": true
    },
    "latitude": {
      "x-data-description": "Latitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true,
      "min": -90,
      "max": 90
    },
    "longitude": {
      "x-data-description": "Longitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true,
      "min": -180,
      "max": 180
    },
    "count": {
      "x-data-description": "Number of Tube Worms",
      "min": 1,
      "type": "integer"
    },
    "cmdrName": {
      "x-data-description": "Commander Name",
      "default": "Dr. Arcanonn",
      "type": "string",
      "required": true
    },
    "cmdrComment": {
      "x-data-description": "Comments",
      "type": "text"
    },
    "isBeta": {
      "x-data-render": false,
      "default": false,
      "type": "boolean"
    },
    "screenshot": {
      "x-data-description": "Screenshot",
      "x-data-type": "image",
      "x-data-render": false,
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "x-data-render": false,
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "updated",
        "verified",
        "accepted",
        "declined",
        "issue"
      ]
    },
    "reportComment": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "text"
    },
    "voteCount": {
      "x-data-render": false,
      "x-data-exclude": true,
      "type": "integer"
    },
    "added": {
      "x-data-render": false,
      "x-data-exclude": true,
      "default": false,
      "type": "boolean"
    },
    "site": {
      "x-data-render": false,
      "x-data-exclude": true,
      "model": "twsite"
    }
  }
}
