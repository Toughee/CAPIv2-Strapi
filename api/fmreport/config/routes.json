{
  "routes": [
    {
      "method": "GET",
      "path": "/fmreports",
      "handler": "Fmreport.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/fmreports/count",
      "handler": "Fmreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/fmreports/:id",
      "handler": "Fmreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/fmreports",
      "handler": "Fmreport.create",
      "config": {
        "policies": [
          "global::block-client",
          "global::block-cmdr",
          "global::report-validation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/fmreports/:id",
      "handler": "Fmreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/fmreports/:id",
      "handler": "Fmreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
