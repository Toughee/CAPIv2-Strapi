{
  "connection": "users",
  "collectionName": "authtokens",
  "info": {
    "name": "Auth Token"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "token": {
      "minLength": 24,
      "required": true,
      "unique": true,
      "type": "string"
    },
    "comment": {
      "type": "string"
    },
    "user": {
      "model": "user",
      "via": "authtokens",
      "plugin": "users-permissions"
    }
  }
}
