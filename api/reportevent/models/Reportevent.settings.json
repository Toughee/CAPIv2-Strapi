{
  "connection": "default",
  "collectionName": "reportevents",
  "info": {
    "name": "reportevent",
    "description": "Journal Event logs, Staging model for tracking changes in the Journal"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "eventName": {
      "type": "string"
    },
    "cmdrName": {
      "type": "string"
    },
    "systemName": {
      "type": "string"
    },
    "bodyName": {
      "type": "string"
    },
    "stationName": {
      "type": "string"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "isBeta": {
      "type": "boolean"
    },
    "clientVersion": {
      "required": true,
      "type": "string"
    },
    "rawJson": {
      "type": "json",
      "required": true
    }
  }
}
