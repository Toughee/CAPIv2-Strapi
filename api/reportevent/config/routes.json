{
  "routes": [
    {
      "method": "GET",
      "path": "/reportevents",
      "handler": "Reportevent.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/reportevents/count",
      "handler": "Reportevent.count",
      "config": {
        "policies": []
      }
    },
    {
