{
  "connection": "default",
  "collectionName": "msreports",
  "info": {
    "name": "msreport",
    "description": "Reporting for Megaships"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "userType": {
      "default": "console",
      "type": "enumeration",
      "required": true,
      "enum": [
        "console",
        "pc"
      ]
    },
    "reportType": {
      "default": "new",
      "type": "enumeration",
      "required": true,
      "enum": [
        "new",
        "update",
        "error"
      ]
    },
    "systemName": {
      "type": "string",
      "required": true
    },
    "systemAddress": {
      "type": "float"
    },
    "regionID": {
      "type": "integer"
    },
    "coordX": {
      "type": "float"
    },
    "coordY": {
      "type": "float"
    },
    "coordZ": {
      "type": "float"
    },
    "orbitBody": {
      "type": "string"
    },
    "shipName": {
      "type": "string",
      "required": true
    },
    "shipTag": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "flightOps": {
      "type": "boolean"
    },
    "flightSchedule": {
      "type": "boolean"
    },
    "dockable": {
      "type": "boolean"
    },
    "cmdrName": {
      "type": "string",
      "required": true
    },
    "cmdrComment": {
      "type": "text"
    },
    "isBeta": {
      "default": false,
      "type": "boolean"
    },
    "screenshot": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "updated",
        "verified",
        "accepted",
        "declined",
        "issue"
      ]
    },
    "reportComment": {
      "type": "text"
    },
    "voteCount": {
      "type": "integer"
    },
    "added": {
      "default": false,
      "type": "boolean"
    },
    "site": {
      "model": "mssite"
    }
  }
}
