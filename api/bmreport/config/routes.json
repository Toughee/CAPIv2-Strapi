{
  "routes": [
    {
      "method": "GET",
      "path": "/bmreports",
      "handler": "Bmreport.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/bmreports/count",
      "handler": "Bmreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/bmreports/:id",
      "handler": "Bmreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/bmreports",
      "handler": "Bmreport.create",
      "config": {
        "policies": [
          "global::block-client",
          "global::block-cmdr",
          "global::report-validation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/bmreports/:id",
      "handler": "Bmreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/bmreports/:id",
      "handler": "Bmreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
