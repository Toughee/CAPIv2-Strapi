{
  "connection": "default",
  "collectionName": "killreports",
  "info": {
    "name": "killreport",
    "description": "Records rewards and kills by CMDR"
  },
  "options": {
    "increments": true,
    "timestamps": false,
    "comment": ""
  },
  "attributes": {
    "systemName": {
      "type": "string"
    },
    "edTimestamp": {
      "type": "datetime"
    },
    "cmdrName": {
      "type": "string"
    },
    "reward": {
      "type": "integer"
    },
    "rewardingFaction": {
      "type": "string"
    },
    "victimFaction": {
      "type": "string"
    },
    "rawEvent": {
      "type": "json"
    },
    "isBeta": {
      "type": "boolean"
    },
    "clientVersion": {
      "required": true,
      "type": "string"
    },
    "reportAccepted": {
      "type": "boolean"
    }
  }
}
