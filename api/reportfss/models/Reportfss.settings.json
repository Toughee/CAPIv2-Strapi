{
  "connection": "default",
  "collectionName": "reportfsses",
  "info": {
    "name": "Report FSS"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "cmdrName": {
      "required": true,
      "type": "string"
    },
    "systemName": {
      "required": true,
      "type": "string"
    },
    "systemAddress": {
      "default": "",
      "type": "biginteger"
    },
    "signalName": {
      "default": "",
      "type": "string"
    },
    "signalNameLocalised": {
      "default": "",
      "type": "string"
    },
    "ussType": {
      "default": "",
      "type": "string"
    },
    "ussTypeLocalised": {
      "default": "",
      "type": "string"
    },
    "spawningState": {
      "default": "",
      "type": "string"
    },
    "spawningStateLocalised": {
      "default": "",
      "type": "string"
    },
    "spawningFaction": {
      "default": "",
      "type": "string"
    },
    "threatLevel": {
      "default": "",
      "type": "integer"
    },
    "isBeta": {
      "default": false,
      "type": "boolean"
    },
    "clientVersion": {
      "required": true,
      "type": "string"
    },
    "reportStatus": {
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "updated",
        "verified",
        "accepted",
        "declined",
        "issue",
        "duplicate"
      ]
    },
    "rawJson": {
      "type": "json",
      "required": true
    }
  }
}

