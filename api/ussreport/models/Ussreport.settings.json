{
  "connection": "default",
  "collectionName": "ussreports",
  "info": {
    "name": "ussreport",
    "description": "Automated journal reports for Unknown Signal Sources"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "systemName": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "threatLevel": {
      "min": 1,
      "max": 10,
      "type": "integer"
    },
    "cmdrName": {
      "type": "string"
    },
    "cmdrComment": {
      "type": "text"
    },
    "isBeta": {
      "default": false,
      "type": "boolean"
    },
    "screenshot": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "updated",
        "accepted",
        "declined",
        "issue"
      ]
    },
    "reportComment": {
      "type": "text"
    },
    "added": {
      "default": false,
      "type": "boolean"
    },
    "site": {
      "model": "usssite"
    },
    "ussRawJson": {
      "type": "json"
    }
  }
}
