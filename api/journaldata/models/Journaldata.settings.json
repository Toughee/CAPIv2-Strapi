{
  "connection": "default",
  "collectionName": "journaldata",
  "info": {
    "name": "journaldata",
    "description": "Raw journal dumps"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "eventName": {
      "type": "string"
    },
    "systemName": {
      "type": "string"
    },
    "bodyName": {
      "type": "string"
    },
    "stationName": {
      "type": "string"
    },
    "cmdrName": {
      "type": "string"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "isBeta": {
      "type": "boolean"
    },
    "clientVersion": {
      "required": true,
      "type": "string"
    },
    "jsonData": {
      "type": "json",
      "required": true
    }
  }
}
