{
  "connection": "default",
  "collectionName": "hdsites",
  "info": {
    "name": "hdsite",
    "description": "Thargoid Hyperdiction sites"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "location": {
      "model": "location",
      "via": "hdSite"
    },
    "toSystem": {
      "model": "system"
    },
    "thargoidCount": {
      "min": 1,
      "type": "integer"
    },
    "isHostile": {
      "type": "boolean"
    },
    "discoveredBy": {
      "model": "cmdr"
    }
  }
}
