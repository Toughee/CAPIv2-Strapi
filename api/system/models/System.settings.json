{
  "connection": "default",
  "collectionName": "systems",
  "info": {
    "name": "system",
    "description": "Systems stored in the Canonn database"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "systemName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "id64": {
      "type": "float"
    },
    "edsmID": {
      "type": "float"
    },
    "edsmCoordX": {
      "type": "float"
    },
    "edsmCoordY": {
      "type": "float"
    },
    "edsmCoordZ": {
      "type": "float"
    },
    "edsmCoordLocked": {
      "type": "boolean"
    },
    "missingSkipCount": {
      "default": 0,
      "type": "integer"
    },
    "bodies": {
      "collection": "body",
      "via": "system"
    }
  }
}
