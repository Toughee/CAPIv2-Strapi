{
  "routes": [
    {
      "method": "GET",
      "path": "/hdreports",
      "handler": "Hdreport.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/hdreports/count",
      "handler": "Hdreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/hdreports/:id",
      "handler": "Hdreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/hdreports",
      "handler": "Hdreport.create",
      "config": {
        "policies": ["global::block-client", "global::block-cmdr"]
      }
    },
    {
      "method": "PUT",
      "path": "/hdreports/:id",
      "handler": "Hdreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/hdreports/:id",
      "handler": "Hdreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
