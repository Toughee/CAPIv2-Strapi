{
  "connection": "default",
  "collectionName": "reportcodices",
  "info": {
    "name": "Report Codex"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "cmdrName": {
      "required": true,
      "type": "string"
    },
    "systemName": {
      "required": true,
      "type": "string"
    },
    "bodyName": {
      "type": "string"
    },
    "systemAddress": {
      "type": "biginteger"
    },
    "coordX": {
      "type": "float"
    },
    "coordY": {
      "type": "float"
    },
    "coordZ": {
      "type": "float"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "frontierID": {
      "type": "integer"
    },
    "entryId": {
      "type": "integer"
    },
    "codexName": {
      "type": "string"
    },
    "codexNameLocalised": {
      "type": "string"
    },
    "subCategory": {
      "type": "string"
    },
    "subCategoryLocalised": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "categoryLocalised": {
      "type": "string"
    },
    "regionName": {
      "type": "string"
    },
    "regionLocalised": {
      "type": "string"
    },
    "voucherAmount": {
      "type": "integer"
    },
    "isBeta": {
      "default": false,
      "type": "boolean"
    },
    "clientVersion": {
      "required": true,
      "type": "string"
    },
    "reportStatus": {
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": ["pending", "updated", "verified", "accepted", "declined", "issue", "duplicate"]
    },
    "rawJson": {
      "type": "json",
      "required": true
    }
  }
}
