{
  "connection": "default",
  "collectionName": "gsreports",
  "info": {
    "name": "gsreport",
    "description": "Reporting for Guardian Structures"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "userType": {
      "default": "console",
      "type": "enumeration",
      "required": true,
      "enum": [
        "console",
        "pc"
      ]
    },
    "reportType": {
      "default": "new",
      "type": "enumeration",
      "required": true,
      "enum": [
        "new",
        "update",
        "error"
      ]
    },
    "systemName": {
      "type": "string",
      "required": true
    },
    "systemAddress": {
      "type": "biginteger"
    },
    "regionID": {
      "type": "integer"
    },
    "coordX": {
      "type": "float"
    },
    "coordY": {
      "type": "float"
    },
    "coordZ": {
      "type": "float"
    },
    "bodyName": {
      "type": "string",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true,
      "min": -90,
      "max": 90
    },
    "longitude": {
      "type": "float",
      "required": true,
      "min": -180,
      "max": 180
    },
    "type": {
      "enum": [
        "Unknown",
        "Lacrosse",
        "Crossroads",
        "Fistbump",
        "Hammerbot",
        "Bear",
        "Bowl",
        "Turtle"
      ],
      "type": "enumeration",
      "required": true
    },
    "hasDatabank": {
      "type": "boolean"
    },
    "frontierID": {
      "type": "integer"
    },
    "cmdrName": {
      "type": "string",
      "required": true
    },
    "cmdrComment": {
      "type": "text"
    },
    "isBeta": {
      "default": false,
      "type": "boolean"
    },
    "clientVersion": {
      "required": true,
      "type": "string"
    },
    "screenshot": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "updated",
        "verified",
        "accepted",
        "declined",
        "issue",
        "duplicate"
      ]
    },
    "reportComment": {
      "type": "text"
    },
    "voteCount": {
      "type": "integer"
    },
    "added": {
      "default": false,
      "type": "boolean"
    },
    "site": {
      "model": "gssite"
    },
    "gsobeliskreport": {
      "collection": "gsobeliskreport",
      "via": "gsreport"
    }
  }
}
