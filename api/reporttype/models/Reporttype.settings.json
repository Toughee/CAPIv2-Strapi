{
  "connection": "default",
  "collectionName": "reporttypes",
  "info": {
    "name": "reporttype",
    "description": "Report endpoints used by Canonn Plugins"
  },
  "options": {
    "increments": true,
    "timestamps": false,
    "comment": ""
  },
  "attributes": {
    "type": {
      "unique": true,
      "type": "string",
      "required": true
    },
    "location": {
      "default": "body",
      "enum": [
        "body",
        "orbital"
      ],
      "type": "enumeration"
    },
    "journalName": {
      "default": "",
      "type": "string"
    },
    "journalID": {
      "default": "",
      "type": "integer"
    },
    "journalType": {
      "default": "codex",
      "enum": [
        "codex",
        "event",
        "fss"
      ],
      "type": "enumeration"
    },
    "endpoint": {
      "default": "",
      "type": "string",
      "required": true
    }
  }
}
