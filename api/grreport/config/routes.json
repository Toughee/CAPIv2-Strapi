{
  "routes": [
    {
      "method": "GET",
      "path": "/grreports",
      "handler": "Grreport.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/grreports/count",
      "handler": "Grreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/grreports/:id",
      "handler": "Grreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/grreports",
      "handler": "Grreport.create",
      "config": {
        "policies": [
          "global::block-client",
          "global::block-cmdr",
          "global::report-validation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/grreports/:id",
      "handler": "Grreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/grreports/:id",
      "handler": "Grreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
